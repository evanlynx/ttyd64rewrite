#new:Header $Header
{
	[MainScript] $Script_Main
	[Background] 80200000
	[EntryList] $EntryList
	[EntryCount] 3
    [MapTattle]  $Function_GetTattle
}

#new:EntryList $EntryList
{
	~Vec4f:Entry0
	~Vec4f:Entry1
	~Vec4f:Entry2
}

#new:Function_Init $Function_Init
{
	PUSH    RA
	LIA     A0, 800B0CF0
	LIA     A1, "tst_tex"
	JAL     800654F0 % sprintf
	RESERVED
	CLEAR  V0
	JPOP   RA
}

#new:Script_Main $Script_Main
{
	Set   *GB_WorldLocation  .Location:KoopaVillage
	Call  SetCamPerspective  ( .Cam:Default 00000003 00000019 00000010 00001000 )
	Call  SetCamBGColor      ( .Cam:Default 00000000 00000000 00000000 )
	Call  SetCamEnabled      ( .Cam:Default .True )
	Call  SetCamLeadPlayer   ( .Cam:Default .False )
	Exec  $Script_SetupMusic
	Exec  $Script_SetupTexturePan
      Exec  $Script_MakeEntities
	Exec  $Script_EnterMap
      Call  MakeNpcs ( .False $NpcGroupList_tyd01 )
      Set  *VarA $Script_CreateExitTriggers
      Exec $Script_EnterVerticalPipe
	Return
	End
}

#new:Script $Script_SetupMusic
{
	Call  SetMusicTrack ( 00000000 000000A7 00000000 00000008 )
	Call  ClearAmbientSounds ( 250` )
	Return
	End
}

#new:Script $Script_EnterMap
{
      Exec  $Script_BindFoliage
      Call  MakeItemEntity ( .Item:StarPiece ~Vec3d:reward1 .ItemSpawnMode:Fixed_NeverVanish *GF_TYD01_Item )
      Bind  $Script_Cane .Trigger:WallHammer ~Collider:candyCane 00000001 00000000
	Return
	End
}

#new:Script $Script_Cane
{
    % Down
    Call    MakeLerp ( 0 -35` 1 .Easing:Linear )
    Loop
        Call    UpdateLerp
        Call    TranslateModel ( ~Model:cane 0 *Var0 0 )
        Wait    1
        If *Var1 == .False
            BreakLoop
        EndIf
    EndLoop
    Add   *MapVar[0] 1
    If *MapVar[0] == 10`
        Call  MakeItemEntity ( .Item:Horsetail ~Vec3d:Item_Horsetail .ItemSpawnMode:BattleReward .False )
    EndIf
    % Up
    Call    MakeLerp ( -35` 0 5 .Easing:Linear )
    Loop
        Call    UpdateLerp
        Call    TranslateModel ( ~Model:cane 0 *Var0 0 )
        Wait    1
        If *Var1 == .False
            BreakLoop
        EndIf
    EndLoop
    Return
    End
}

#new:Script $Script_MakeEntities
{
    Call  MakeEntity        ( .Entity:RedBlock ~Vec4d:RedBlock .Item:CloseCall 80000000 )
    Call  AssignBlockFlag   ( *GF_TYD01_RedBlock )
    Call  MakeEntity        ( .Entity:MultiCoinBrick ~Vec4d:YellowBlock 80000000 )
    Call  AssignBlockFlag   ( *GF_TYD01_CoinBlock )
    Call  MakeEntity        ( .Entity:BrickBlock ~Vec4d:BrickBlock 80000000 )
    Return
    End
}

#new:Script $Script_CreateExitTriggers
{
      Bind  $Script_Exit0 .Trigger:FloorTouch ~Collider:Exit0 00000001 00000000
      Bind  $Script_Exit1 .Trigger:FloorTouch ~Collider:Exit1 00000001 00000000
      Bind  $Script_Exit2 .Trigger:FloorTouch ~Collider:Exit2 00000001 00000000
	Return
	End
}

#new:Script $Script_Exit0
{
      SetGroup 0000001B
      Set   *VarA ~Entry:Entry0
      Set   *VarB ~Collider:Exit0
      Set   *VarC $Script_GoToMap_Exit0
      ExecWait  $Script_ExitVerticalPipe
      Return
      End
}

#new:Script $Script_Exit1
{
      SetGroup 0000001B
	Set   *VarA ~Entry:Entry1
	Set   *VarB ~Collider:Exit1
	Set   *VarC $Script_GoToMap_Exit1
	ExecWait  $Script_ExitVerticalPipe
	Return
	End
}

#new:Script $Script_Exit2
{
      SetGroup 0000001B
	Set   *VarA ~Entry:Entry2
	Set   *VarB ~Collider:Exit2
	Set   *VarC $Script_GoToMap_Exit2
	ExecWait  $Script_ExitVerticalPipe
	Return
	End
}

#new:Script $Script_GoToMap_Exit0
{
    Call  GotoMap   ( "nok_13" 00000003 )
    Wait  100`
    Return
    End
}

#new:Script $Script_GoToMap_Exit1
{
    Call  GotoMap   ( "tyd_01" 00000002 )
    Wait  100`
    Return
    End
}

#new:Script $Script_GoToMap_Exit2
{
    Call  GotoMap   ( "tyd_01" 00000001 )
    Wait  100`
    Return
    End
}

