#new:Script $Script_RespawnPlayer
{
    Call    DisablePlayerInput ( .True )
    Call    PlaySound ( 00D9 )
    Call 	$WriteAddress ( 8015C790 2 D0D0D000 .False .False ) % set fade color as D0D0D0 (White)(RGB)
    SetF	*Var0 *Fixed[25.0] % speed
    SetF	*Var1 *Fixed[255.0]  % dark
    ExecWait $Script_FadeScreenOut
    Call    SetNpcPos ( .Npc:Partner ~Vec3d:Respawn )
    Wait    1
    Call    ResetCam ( .Cam:Default 100` )
    Wait    20`
    Call    PlaySound ( 00DA )
    SetF	*Var0 *Fixed[25.0] % speed
    SetF	*Var1 *Fixed[5.0]  % fully bright
    ExecWait $Script_FadeScreenIn
    Call    DisablePlayerInput ( .False )
    Call 	$WriteAddress ( 8015C790 2 .False .False .False ) % restore color to black
    Set     *MapVar[0] .False
    Return
    End
}

