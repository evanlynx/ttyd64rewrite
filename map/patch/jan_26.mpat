#new:Header $Header
{
	[MainScript] $Script_Main
	[Background] 80200000
	[EntryList] $EntryList
	[EntryCount] 2
    [MapTattle]  $Function_GetTattle
}

#new:EntryList $EntryList
{
	~Vec4f:Entry0
    ~Vec4f:Entry1
}

#new:Script_Main $Script_Main
{
	Set   *GB_WorldLocation  .Location:JadeJungle
	Call  SetCamPerspective  ( .Cam:Default 00000003 00000019 00000010 00001000 )
	Call  SetCamBGColor      ( .Cam:Default 00000000 00000000 00000000 )
	Call  SetCamEnabled      ( .Cam:Default .True )
	Call  SetCamLeadPlayer   ( .Cam:Default .False )
	Exec  $Script_MakeEntities
	Exec  $Script_SetupMusic
	Exec  $Script_SetupTexturePan
    If *GF_JAN26_Shield == .True
        Call    EnableGroup ( ~Model:Shield .False )
        Call    ModifyColliderFlags 	( .False ~Collider:ravenStatue1 7FFFFE00 )
        Call    ModifyColliderFlags 	( .False ~Collider:ravenStatue2 7FFFFE00 )
        If *GF_JAN26_Reward == .False
            Bind  $Script_GetItem .Trigger:WallPressA ~Collider:reward 00000001 00000000
        EndIf
    Else
        Bind  $Script_DestroyRaven_1 .Trigger:WallHammer ~Collider:ravenStatue1 00000001 00000000
        Bind  $Script_DestroyRaven_2 .Trigger:WallHammer ~Collider:ravenStatue2 00000001 00000000
        Bind  $Script_RespawnMain .Trigger:FloorTouch ~Collider:rMain 00000001 00000000
        Bind  $Script_Respawn_1 .Trigger:FloorTouch ~Collider:r1 00000001 00000000
        Bind  $Script_Respawn_2 .Trigger:FloorTouch ~Collider:r2 00000001 00000000
        Bind  $Script_Respawn_3 .Trigger:FloorTouch ~Collider:r3 00000001 00000000
        Bind  $Script_Respawn_4 .Trigger:FloorTouch ~Collider:r4 00000001 00000000
        Bind  $Script_Respawn_5 .Trigger:FloorTouch ~Collider:r5 00000001 00000000
        Bind  $Script_ClearMapVar0 .Trigger:FloorTouch ~Collider:water 00000001 00000000
    EndIf
    Bind  $Script_Sign1 .Trigger:FloorAbove ~Collider:sign1 00000001 00000000
    Bind  $Script_Sign2 .Trigger:WallPressA ~Collider:sign2 00000001 00000000
    % Sushie Docks
    Call     ModifyColliderFlags 	( 00000003 ~Collider:port1 00000004 )
    Call     ModifyColliderFlags 	( 00000003 ~Collider:port2 00000004 )
    Call     ModifyColliderFlags 	( 00000003 ~Collider:port3 00000004 )
    %
    Call     ModifyColliderFlags 	( 00000000 ~Collider:port1 00080000 )
    Call     ModifyColliderFlags 	( 00000000 ~Collider:port2 00080000 )
    Call     ModifyColliderFlags 	( 00000000 ~Collider:port3 00080000 )
    %
    Call    MakeItemEntity ( .Item:DefendPlusP ~Vec3d:Badge .ItemSpawnMode:Fixed_NeverVanish *GF_JAN26_Reward )
    Call    MakeNpcs ( .False $NpcGroupList_jan26 )
    Set     *MapVar[1] *Var0 % save itemEntityIndex
	Set   *Var0 $Script_CreateExitTriggers
    Exec  EnterWalk
	Return
	End
}

#new:Script $Script_GetItem
{
    Call    DisablePlayerInput ( .True )
    Call    AddBadge ( .Item:DefendPlusP 00000000 )
    Call    RemoveItemEntity ( *MapVar[1] )
    Call    ShowGotItem ( .Item:DefendPlusP .True 00000000 )
    Set     *GF_JAN26_Reward .True
    Unbind
    Call    DisablePlayerInput ( .False )
    Return
    End
}

% Ravens

#new:Script $Script_DestroyRaven_1
{
    Set     *Var0 ~Model:ravenStatue1
    Set     *Var1 ~Collider:ravenStatue1
    Set     *MapFlag[0] .True
    ExecWait $Script_DestroyRaven
    Return
    End
}

#new:Script $Script_DestroyRaven_2
{
    Set     *Var0 ~Model:ravenStatue2
    Set     *Var1 ~Collider:ravenStatue2
    Set     *MapFlag[1] .True
    ExecWait $Script_DestroyRaven
    Return
    End
}

#new:Script $Script_DestroyRaven
{
    SetGroup 0
    SuspendAll 2
    Set     *Var3 *Var0
    Set     *Var4 *Var1
    Call    DisablePlayerInput ( .True )
    Call    GetModelCenter ( *Var3 )
    Sub     *Var1 1 % y
    Add     *Var2 17` % z
    Call    PlayEffect    ( ~FX:RingBlast:Yellow *Var0 *Var1 *Var2 *Fixed[1.2] 20` .False .False .False .False .False .False .False )
    Wait    2
    Call    GetModelCenter ( *Var3 )
    Sub     *Var1 15` % y
    Call    PlayEffect    ( ~FX:PinkShimmer:Spiral *Var0 *Var1 *Var2 30` 20` 5 00000096 .False .False .False .False .False )
    Call    PlaySound ( 2051 )
    Wait    10`
    Call    EnableModel ( *Var3 .False )
    Wait    10`
    Set     *Var0 *MapFlag[0]
    Add     *Var0 *MapFlag[1]
    If *Var0 >= 2
        Call    ShowMessageAtScreenPos ( $String_ShieldDestroyed A0 28 )
        Set     *GF_JAN26_Shield .True
        Call    EnableModel ( ~Model:shield .False )
        Bind    $Script_GetItem .Trigger:WallPressA ~Collider:reward 00000001 00000000
    EndIf
    ResumeAll  2
    Call    ModifyColliderFlags ( .False *Var4 7FFFFE00 )
    Call    DisablePlayerInput ( .False )
    Return
    End
}

% Pillars

#new:Script $Script_RespawnMain
{
    Set *Var0 *GF_JAN26_Shield
    Add *Var0 *MapFlag[0]
    If *Var0 > .False
        Unbind
        Return
    EndIf
    ExecWait $Script_RespawnPlayer
    Return
    End
}

#new:Script $Script_ClearMapVar0
{
    Set *MapVar[0] .False
    % Close any active dialog
    Call $ReadAddress ( 80155128 0 *Var0 .False .False )
    If *Var0 == 5 % message on screen
        Call 	$WriteAddress ( 80155128 0 3 .False .False ) % close active dialog bubble
        Call    $WriteAddress ( $FFWD 2 .False .False .False ) % disable FFWD
    EndIf
    Return
    End
}

#new:Script $Script_Respawn_1
{
    % Close any active dialog
    Call $ReadAddress ( 80155128 0 *Var0 .False .False )
    If *Var0 == 5 % message on screen
        Call 	$WriteAddress ( 80155128 0 3 .False .False ) % close active dialog bubble
        Call    $WriteAddress ( $FFWD 2 .False .False .False ) % disable FFWD
    EndIf
    %
    Set *Var0 *GF_JAN26_Shield
    Add *Var0 *MapFlag[0]
    If *Var0 > .False
        Unbind
        Return
    EndIf
    Set *MapVar[0] 1
    Return
    End
}

#new:Script $Script_Respawn_2
{
    Set *Var0 *GF_JAN26_Shield
    Add *Var0 *MapFlag[0]
    If *Var0 > .False
        Unbind
        Return
    EndIf
    Switch *MapVar[0]
        CaseOR == 1
        CaseOR == 2
            Set *MapVar[0] 2
        EndCaseGroup
        Default
            ExecWait $Script_RespawnPlayer
    EndSwitch
    Return
    End
}

